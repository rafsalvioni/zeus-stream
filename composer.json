{
    "name": "rafsalvioni/zeus-stream",
    "description": "provides a api to manage streams",
    "license": "BSD-3-Clause",
    "keywords": [
        "stream",
        "zeus"
    ],
    "homepage": "https://github.com/rafsalvioni/zeus-stream",
    "autoload": {
        "psr-4": {
            "Zeus\\Stream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZeusTest\\Stream\\": "test/"
        }
    },
    "require": {
        "php": ">=5.5 <6",
        "rafsalvioni/zeus-base": "1.0.0",
        "rafsalvioni/zeus-event": "1.0.0",
        "rafsalvioni/zeus-core": "1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../zeus-*"
        }
    ],    
    "suggest": {
    },
    "extra": {
    }
}
